[github]
        user = awaxa
[core]
	excludesfile = ~/.gitglobalignore
[color]
        diff = auto
        status = auto
        branch = auto
[alias]
        a = add
        all = add -A
        b = branch
        bv = branch -av
        c = checkout
        co = checkout
        m = commit -m
        am = commit -am
        d = diff
        ds = diff --stat
        l = log --oneline
	l1 = log --pretty=oneline
        ll = log
        s = status
