[user]
	name = awaxa
	email = awaxa@awaxa.com
[github]
        user = awaxa
[core]
	excludesfile = ~/.gitglobalignore
[color]
        diff = auto
        status = auto
        branch = auto
[alias]
        a = add
        all = add -A
        b = branch
        bv = branch -av
        co = checkout
        c = commit
        dc = commit --dry-run
        m = commit -m
        am = commit -am
        d = diff
        ds = diff --stat
        l = log --oneline
        l1 = log --pretty=oneline
        ll = log
        s = status
